{% extends "../../shared/layout/mdefaultLayou.njk" %}
{% from "../../shared/sharedView/macro/wuc/mheader.njk" import mheader,
smheader %}
{% block title %}
发布口碑 - 产品口碑 - 乐品包装
{% endblock title %}

{% block keywords %}{% endblock %}

{% block description %}{% endblock %}

{% block style %}
  <link href="/dist/styles/m/reputation/publish.css" rel="stylesheet">

{% endblock style %}

{% block script %}
  <script type="text/javascript">
    var companyId = {{companyId}}
    var productId = {{productId}}
  </script>

  <script type="text/javascript" src="/dist/scripts/m/reputation/publish.js"></script>

{% endblock script %}

{% block body %}
  <div class="header">
    {{mheader({title:'发布口碑'})}}
    <div class="submit">发布</div>
  </div>

  <div class="selectpooduct clearfix">
    <img _src_="/assets/images/empty.png"/>
    <i class="iconfont_wlzj">&#xE00C;</i>
    <div class="c">
      <h3>阿斯蒂芬</h3>
      <span>爱迪生发大</span>
    </div>
  </div>

  <div class="info">
    <div class="company clearfix">
      <span>购买时间</span>
      <i class="iconfont_wlzj">&#xE00C;</i>
      <input type="text" placeholder="请输入购买时间" readonly="readonly" class="more">
    </div>
    <div class="company clearfix">
      <span>使用时间</span>
      <i class="iconfont_wlzj">&#xE00C;</i>
      <input type="text" placeholder="请输入使用时间" readonly="readonly" class="more">
    </div>
    <div class="company clearfix">
      <span>购买价格</span>
      <input type="text" placeholder="请输入购买价格">
    </div>
  </div>

{% endblock body %}